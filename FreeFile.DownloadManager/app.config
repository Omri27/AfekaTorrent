<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_FilesService">
                    <security mode="None" />
                </binding>
                <binding name="NetTcpBinding_UserService">
                    <security mode="None" />
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://10.100.102.8/files/" binding="netTcpBinding"
